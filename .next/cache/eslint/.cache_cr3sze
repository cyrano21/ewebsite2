 setNewOrderData] = useState({\n    customer: '',\n    email: '',\n    itemsJson: '[]',\n    total: '',\n    status: 'En attente',\n    paymentMethod: 'Carte de crdit',\n    shippingAddress: ''\n  });\n  \n  // tats pour filtres avancs\n  const [filterData, setFilterData] = useState({ dateFrom: '', dateTo: '', minTotal: '', maxTotal: '' });\n  \n  // Rcuprer le paramtre customer de l'URL si prsent\n  useEffect(() => {\n    if (router.query.customer) {\n      setSearchTerm(router.query.customer);\n    }\n  }, [router.query]);\n\n  // Charger les commandes depuis l'API\n  // eslint-disable-next-line react-hooks/exhaustive-deps\n  useEffect(() => {\n    setIsLoading(true);\n    \n    // utilsation de l'API Next.js\n    fetch('/api/orders')\n      .then(response => response.json())\n      .then(data => {\n        setOrders(data.orders);\n        setIsLoading(false);\n      })\n      .catch(error => {\n        console.error('Erreur lors du chargement des commandes:', error);\n        // En cas d'erreur, utilser des donnes factices pour dmonstration\n        setOrders(orderData);\n        setIsLoading(false);\n      });\n  }, []);\n\n  // Donnes factices pour dmonstration\n  const orderData = [\n    { \n      id: 'ORD-1234', \n      customer: 'Jean Dupont', \n      customerId: 1,\n      email: 'jean.dupont@example.com',\n      date: '19/04/2025', \n      amount: 129.99, \n      status: 'Livr',\n      paymentMethod: 'Carte de crdit',\n      items: [\n        { name: 'Nike Premier X', quantity: 1, price: 129.99 }\n      ],\n      address: {\n        street: '123 Rue de Paris',\n        city: 'Paris',\n        zipCode: '75001',\n        country: 'France'\n      }\n    },\n    { \n      id: 'ORD-1235', \n      customer: 'Marie Dubois', \n      customerId: 2,\n      email: 'marie.dubois@example.com',\n      date: '18/04/2025', \n      amount: 89.50, \n      status: 'En cours',\n      paymentMethod: 'PayPal',\n      items: [\n        { name: 'Asthetic Bags', quantity: 1, price: 89.50 }\n      ],\n      address: {\n        street: '45 Avenue des Champs',\n        city: 'Lyon',\n        zipCode: '69000',\n        country: 'France'\n      }\n    },\n    { \n      id: 'ORD-1236', \n      customer: 'Pierre Martin', \n      customerId: 3,\n      email: 'pierre.martin@example.com',\n      date: '17/04/2025', \n      amount: 45.00, \n      status: 'Annul',\n      paymentMethod: 'Carte de crdit',\n      items: [\n        { name: 'Woolen T-shirt', quantity: 1, price: 45.00 }\n      ],\n      address: {\n        street: '78 Boulevard des Fleurs',\n        city: 'Marseille',\n        zipCode: '13000',\n        country: 'France'\n      }\n    },\n    { \n      id: 'ORD-1237', \n      customer: 'Sophie Bernard', \n      customerId: 4,\n      email: 'sophie.bernard@example.com',\n      date: '16/04/2025', \n      amount: 245.80, \n      status: 'Expdi',\n      paymentMethod: 'PayPal',\n      items: [\n        { name: 'Nike Air Force 1', quantity: 1, price: 149.99 },\n        { name: 'Cotton T-shirt', quantity: 2, price: 47.90 }\n      ],\n      address: {\n        street: '12 Rue du Commerce',\n        city: 'Bordeaux',\n        zipCode: '33000',\n        country: 'France'\n      }\n    },\n    { \n      id: 'ORD-1238', \n      customer: 'Lucas Petit', \n      customerId: 5,\n      email: 'lucas.petit@example.com',\n      date: '15/04/2025', \n      amount: 75.50, \n      status: 'En attente',\n      paymentMethod: 'Carte de crdit',\n      items: [\n        { name: 'Sac  dos Aventure', quantity: 1, price: 75.50 }\n      ],\n      address: {\n        street: '3 Avenue Rpublique',\n        city: 'Toulouse',\n        zipCode: '31000',\n        country: 'France'\n      }\n    }\n  ];\n\n  // Filtrer les commandes en fonction du terme de recherche et du statut slectionn\n  const filteredOrders = orders.filter((order) => {\n    const matchesSearch = order.id.toString().includes(searchTerm) ||\n      order.customer.toLowerCase().includes(searchTerm.toLowerCase()) ||\n      order.email.toLowerCase().includes(searchTerm.toLowerCase()) ||\n      (router.query.customer && order.customerId.toString() === router.query.customer);\n    const matchesStatus = selectedStatus === 'All' || order.status === selectedStatus;\n    // Filtres date et montant\n    const orderDate = new Date(order.date);\n    const matchesDate = (!filterData.dateFrom || orderDate >= new Date(filterData.dateFrom)) &&\n                        (!filterData.dateTo || orderDate <= new Date(filterData.dateTo));\n    const matchesAmount = (!filterData.minTotal || order.amount >= parseFloat(filterData.minTotal)) &&\n                          (!filterData.maxTotal || order.amount <= parseFloat(filterData.maxTotal));\n    return matchesSearch && matchesStatus && matchesDate && matchesAmount;\n  });\n\n  // Calculer les indices pour la pagination\n  const indexOfLastItem = currentPage * itemsPerPage;\n  const indexOfFirstItem = indexOfLastItem - itemsPerPage;\n  const currentItems = filteredOrders.slice(indexOfFirstItem, indexOfLastItem);\n\n  // Changer de page\n  const paginate = (pageNumber) => setCurrentPage(pageNumber);\n\n  // Ouvrir le modal avec les dtails de la commande\n  const openOrderDetails = (order) => {\n    setCurrentOrder(order);\n    setShowModal(true);\n  };\n\n  // Gestionnaire de recherche\n  const handleSearch = (e) => {\n    setSearchTerm(e.target.value);\n    setCurrentPage(1); // Rinitialiser  la premire page lors d'une recherche\n  };\n\n  // Gestionnaire de changement de statut\n  const handleStatusChange = (e) => {\n    setSelectedStatus(e.target.value);\n    setCurrentPage(1); // Rinitialiser  la premire page lors d'un changement de statut\n  };\n\n  // Mise  jour du statut d'une commande\n  const updateOrderStatus = async (orderId, newStatus) => {\n    try {\n      const response = await fetch(`/api/orders/${orderId}`, {\n        method: 'PATCH',\n        headers: {\n          'Content-Type': 'application/json',\n        },\n        body: JSON.stringify({ status: newStatus }),\n      });\n      \n      if (response.ok) {\n        // Mettre  jour l'tat local\n        const updatedOrders = orders.map(order => \n          order.id === orderId ? { ...order, status: newStatus } : order\n        );\n        setOrders(updatedOrders);\n        \n        // Si la commande en cours de visualisation est celle qui a t mise  jour, mettre  jour galement\n        if (currentOrder && currentOrder.id === orderId) {\n          setCurrentOrder({ ...currentOrder, status: newStatus });\n        }\n      } else {\n        alert('Erreur lors de la mise  jour du statut de la commande');\n      }\n    } catch (error) {\n      console.error('Erreur:', error);\n      alert('Erreur de connexion au serveur');\n    }\n  };\n\n  // Annuler une commande\n  const cancelOrder = async (orderId) => {\n    if (window.confirm('tes-vous sr de vouloir annuler cette commande ?')) {\n      await updateOrderStatus(orderId, 'Annul');\n    }\n  };\n\n  // Gestion cration commande\n  const handleCreateChange = (e) => {\n    const { name, value } = e.target;\n    setNewOrderData(prev => ({ ...prev, [name]: value }));\n  };\n  const submitNewOrder = async (e) => {\n    e.preventDefault();\n    try {\n      const orderPayload = {\n        items: JSON.parse(newOrderData.itemsJson || '[]'),\n        total: parseFloat(newOrderData.total) || 0,\n        status: newOrderData.status,\n        shippingAddress: newOrderData.shippingAddress,\n        paymentMethod: newOrderData.paymentMethod\n      };\n      const response = await fetch('/api/orders', {\n        method: 'POST',\n        headers: { 'Content-Type': 'application/json' },\n        body: JSON.stringify(orderPayload)\n      });\n      const created = await response.json();\n      setOrders(prev => [created, ...prev]);\n      setShowCreateModal(false);\n    } catch (error) {\n      console.error('Erreur cration commande:', error);\n    }\n  };\n\n  // Couleur du badge en fonction du statut\n  const getStatusBadgeVariant = (status) => {\n    switch(status) {\n      case 'Livr': return 'success';\n      case 'Expdi': return 'info';\n      case 'En cours': return 'primary';\n      case 'En attente': return 'warning';\n      case 'Annul': return 'danger';\n      default: return 'secondary';\n    }\n  };\n\n  return (\n    <AdminLayout>\n      <PageHeader title=\"Gestion des commandes\" curPage=\"Admin / Commandes\" />\n      \n      <Container fluid className=\"py-4\">\n        <Card className=\"shadow-sm mb-4\">\n          <Card.Body>\n            <Row className=\"align-items-center mb-3\">\n              <Col>\n                <h5 className=\"mb-0\">Liste des commandes</h5>\n              </Col>\n              <Col xs=\"auto\">\n                <Button variant=\"primary\" size=\"sm\" onClick={() => setShowCreateModal(true)}>\n                  <i className=\"icofont-plus-circle me-1\"></i> Crer une commande\n                </Button>\n              </Col>\n            </Row>\n            \n            <Row className=\"mb-4\">\n              <Col md={6}>\n                <InputGroup>\n                  <Form.Control\n                    placeholder=\"Rechercher par ID, client ou email...\"\n                    value={searchTerm}\n                    onChange={handleSearch}\n                  />\n                  <Button variant=\"outline-secondary\">\n                    <i className=\"icofont-search-1\"></i>\n                  </Button>\n                </InputGroup>\n              </Col>\n              <Col md={3}>\n                <Form.Select value={selectedStatus} onChange={handleStatusChange}>\n                  <option value=\"All\">Tous les statuts</option>\n                  <option value=\"En attente\">En attente</option>\n                  <option value=\"En cours\">En cours</option>\n                  <option value=\"Expdi\">Expdi</option>\n                  <option value=\"Livr\">Livr</option>\n                  <option value=\"Annul\">Annul</option>\n                </Form.Select>\n              </Col>\n            </Row>\n            \n            {/* Filtres avancs inline */}\n            <Row className=\"mb-4 g-3\">\n              <Col md={3}>\n                <Form.Group controlId=\"filterDateFrom\">\n                  <Form.Label>Date de dbut</Form.Label>\n                  <Form.Control type=\"date\" value={filterData.dateFrom} onChange={e => setFilterData(prev => ({ ...prev, dateFrom: e.target.value }))} />\n                </Form.Group>\n              </Col>\n              <Col md={3}>\n                <Form.Group controlId=\"filterDateTo\">\n                  <Form.Label>Date de fin</Form.Label>\n                  <Form.Control type=\"date\" value={filterData.dateTo} onChange={e => setFilterData(prev => ({ ...prev, dateTo: e.target.value }))} />\n                </Form.Group>\n              </Col>\n              <Col md={3}>\n                <Form.Group controlId=\"filterMinTotal\">\n                  <Form.Label>Montant min ()</Form.Label>\n                  <Form.Control type=\"number\" name=\"minTotal\" value={filterData.minTotal} onChange={e => setFilterData(prev => ({ ...prev, minTotal: e.target.value }))} placeholder=\"0\" />\n                </Form.Group>\n              </Col>\n              <Col md={3}>\n                <Form.Group controlId=\"filterMaxTotal\">\n                  <Form.Label>Montant max ()</Form.Label>\n                  <Form.Control type=\"number\" name=\"maxTotal\" value={filterData.maxTotal} onChange={e => setFilterData(prev => ({ ...prev, maxTotal: e.target.value }))} placeholder=\"0\" />\n                </Form.Group>\n              </Col>\n            </Row>\n            \n            {isLoading ? (\n              <div className=\"text-center py-5\">\n                <div className=\"spinner-border text-primary\" role=\"status\">\n                  <span className=\"visually-hidden\">Chargement...</span>\n                </div>\n              </div>\n            ) : (\n              <>\n                <div className=\"table-responsive\">\n                  <Table hover>\n                    <thead>\n                      <tr>\n                        <th>Commande</th>\n                        <th>Client</th>\n                        <th>Date</th>\n                        <th>Total</th>\n                        <th>Statut</th>\n                        <th>Actions</th>\n                      </tr>\n                    </thead>\n                    <tbody>\n                      {currentItems.length > 0 ? (\n                        currentItems.map((order) => (\n                          <tr key={order.id}>\n                            <td>{order.id}</td>\n                            <td>\n                              <div>{order.customer}</div>\n                              <small className=\"text-muted\">{order.email}</small>\n                            </td>\n                            <td>{order.date}</td>\n                            <td>{order.amount.toFixed(2)} </td>\n                            <td>\n                              <Badge bg={getStatusBadgeVariant(order.status)}>\n                                {order.status}\n                              </Badge>\n                            </td>\n                            <td>\n                              <Button \n                                variant=\"link\" \n                                className=\"p-0 me-2\" \n                                onClick={() => openOrderDetails(order)}\n                              >\n                                <i className=\"icofont-eye-alt text-primary\"></i>\n                              </Button>\n                              <Button \n                                variant=\"link\" \n                                className=\"p-0 me-2\"\n                              >\n                                <i className=\"icofont-edit text-secondary\"></i>\n                              </Button>\n                              {order.status !== 'Annul' && order.status !== 'Livr' && (\n                                <Button \n                                  variant=\"link\" \n                                  className=\"p-0\"\n                                  onClick={() => cancelOrder(order.id)}\n                                >\n                                  <i className=\"icofont-ui-delete text-danger\"></i>\n                                </Button>\n                              )}\n                            </td>\n                          </tr>\n                        ))\n                      ) : (\n                        <tr>\n                          <td colSpan=\"6\" className=\"text-center py-4\">\n                            Aucune commande trouve correspondant  votre recherche.\n                          </td>\n                        </tr>\n                      )}\n                    </tbody>\n                  </Table>\n                </div>\n                \n                <div className=\"d-flex justify-content-between align-items-center mt-4\">\n                  <div>\n                    Affichage de {indexOfFirstItem + 1}  {Math.min(indexOfLastItem, filteredOrders.length)} sur {filteredOrders.length} commandes\n                  </div>\n                  \n                  <Pagination>\n                    <Pagination.Prev \n                      onClick={() => setCurrentPage(current => Math.max(current - 1, 1))}\n                      disabled={currentPage === 1}\n                    />\n                    \n                    {[...Array(Math.ceil(filteredOrders.length / itemsPerPage)).keys()].map(number => (\n                      <Pagination.Item\n                        key={number + 1}\n                        active={number + 1 === currentPage}\n                        onClick={() => paginate(number + 1)}\n                      >\n                        {number + 1}\n                      </Pagination.Item>\n                    ))}\n                    \n                    <Pagination.Next \n                      onClick={() => setCurrentPage(current => \n                        Math.min(current + 1, Math.ceil(filteredOrders.length / itemsPerPage))\n                      )}\n                      disabled={currentPage === Math.ceil(filteredOrders.length / itemsPerPage)}\n                    />\n                  </Pagination>\n                </div>\n              </>\n            )}\n          </Card.Body>\n        </Card>\n      </Container>\n      \n      {/* Modal de dtails de la commande */}\n      <Modal show={showModal} onHide={() => setShowModal(false)} size=\"lg\">\n        <Modal.Header closeButton>\n          <Modal.Title>Dtails de la commande {currentOrder?.id}</Modal.Title>\n        </Modal.Header>\n        <Modal.Body>\n          {currentOrder && (\n            <>\n              <Row className=\"mb-4\">\n                <Col>\n                  <div className=\"d-flex justify-content-between align-items-center mb-3\">\n                    <h5 className=\"mb-0\">Rsum</h5>\n                    <Badge bg={getStatusBadgeVariant(currentOrder.status)} className=\"fs-6\">\n                      {currentOrder.status}\n                    </Badge>\n                  </div>\n                  <p className=\"mb-1\">\n                    <strong>Date de commande:</strong> {currentOrder.date}\n                  </p>\n                  <p className=\"mb-1\">\n                    <strong>Mthode de paiement:</strong> {currentOrder.paymentMethod}\n                  </p>\n                  <p className=\"mb-1\">\n                    <strong>Total:</strong> {currentOrder.amount.toFixed(2)} \n                  </p>\n                </Col>\n              </Row>\n              \n              <Row className=\"mb-4\">\n                <Col md={6}>\n                  <h6>Informations client</h6>\n                  <p className=\"mb-1\">{currentOrder.customer}</p>\n                  <p classN