 default: false,\n    },\n    isFeatured: {\n      type: Boolean,\n      default: false,\n    },\n    tags: {\n      type: [String],\n      default: [],\n    },\n  },\n  {\n    timestamps: true,\n  }\n);\n\n// Vrifier si le modle existe dj pour viter les erreurs de redfinition\nconst Product =\n  mongoose.models.Product || mongoose.model(\"Product\", ProductSchema);\n\nexport default async function handler(req, res) {\n  const { id } = req.query;\n\n  if (!id) {\n    return res.status(400).json({ error: \"ID du produit manquant\" });\n  }\n\n  try {\n    // Connexion  MongoDB\n    const conn = await connectDB();\n\n    if (!conn) {\n      return res\n        .status(500)\n        .json({ error: \"Erreur de connexion  la base de donnes\" });\n    }\n\n    switch (req.method) {\n      case \"GET\":\n        // Rcuprer un produit spcifique\n        const product = await Product.findById(id);\n        if (!product) {\n          return res.status(404).json({ error: \"Produit non trouv\" });\n        }\n        return res.status(200).json(product);\n\n      case \"PUT\":\n        // Mettre  jour un produit\n        const updatedProduct = await Product.findByIdAndUpdate(id, req.body, {\n          new: true,\n          runValidators: true,\n        });\n\n        if (!updatedProduct) {\n          return res.status(404).json({ error: \"Produit non trouv\" });\n        }\n        return res.status(200).json(updatedProduct);\n\n      case \"DELETE\":\n        // Supprimer un produit\n        const productToDelete = await Product.findById(id);\n        if (!productToDelete) {\n          return res.status(404).json({ error: \"Produit non trouv\" });\n        }\n\n        // Si le produit a une image Cloudinary, la supprimer galement\n        if (productToDelete.cloudinaryId) {\n          try {\n            await deleteImage(productToDelete.cloudinaryId);\n          } catch (error) {\n            console.error(\"Erreur lors de la suppression de l'image:\", error);\n            // Continuer malgr l'erreur de suppression d'image\n          }\n        }\n\n        await Product.findByIdAndDelete(id);\n        return res\n          .status(200)\n          .json({ message: \"Produit supprim avec succs\" });\n\n      default:\n        return res.status(405).json({ error: \"Mthode non autorise\" });\n    }\n  } catch (error) {\n    console.error(\"Erreur API produit:\", error);\n    return res\n      .status(500)\n      .json({ error: \"Erreur serveur\", details: error.message });\n  }\n}\n","C:\\Users\\Louis Olivier\\Downloads\\ewebsite2-francise\\pages\\api\\reviews\\index.js",["880","881","882","883","884","885"],[],"import connectDB from '../../../src/config/db';\r\nimport mongoose from 'mongoose';\r\nimport { withAuth } from '../../../src/middleware/authMiddleware';\r\n\r\n// Dfinition du schma Review pour MongoDB\r\nconst ReviewSchema = new mongoose.Schema({\r\n  product: {\r\n    type: mongoose.Schema.Types.ObjectId,\r\n    ref: 'Product',\r\n    required: true\r\n  },\r\n  user: {\r\n    type: mongoose.Schema.Types.ObjectId,\r\n    ref: 'User',\r\n    required: true\r\n  },\r\n  rating: {\r\n    type: Number,\r\n    required: true,\r\n    min: 1,\r\n    max: 5\r\n  },\r\n  comment: {\r\n    type: String,\r\n    required: true\r\n  },\r\n  isApproved: {\r\n    type: Boolean,\r\n    default: false\r\n  }\r\n}, {\r\n  timestamps: true\r\n});\r\n\r\n// Vrifier si le modle existe dj pour viter les erreurs de redfinition\r\nconst Review = mongoose.models.Review || mongoose.model('Review', ReviewSchema);\r\n\r\nasync function handler(req, res) {\r\n  try {\r\n    // Connexion  MongoDB\r\n    const conn = await connectDB();\r\n    \r\n    if (!conn) {\r\n      return res.status(500).json({ error: 'Erreur de connexion  la base de donnes' });\r\n    }\r\n    \r\n    switch (req.method) {\r\n      case 'GET':\r\n        // Rcuprer tous les avis ou filtrer par produit\r\n        const { productId, approved } = req.query;\r\n        let query = {};\r\n        \r\n        if (productId) {\r\n          query.product = productId;\r\n        }\r\n        \r\n        if (approved === 'true') {\r\n          query.isApproved = true;\r\n        } else if (approved === 'false') {\r\n          query.isApproved = false;\r\n        }\r\n        \r\n        const reviews = await Review.find(query)\r\n          .populate('user', 'name profileImage')\r\n          .sort({ createdAt: -1 });\r\n          \r\n        return res.status(200).json(reviews);\r\n        \r\n      case 'POST':\r\n        // Crer un nouvel avis\r\n        const { product, rating, comment } = req.body;\r\n        \r\n        // Vrifier si l'utilsateur a dj laiss un avis pour ce produit\r\n        const existingReview = await Review.findOne({\r\n          product,\r\n          user: req.user._id\r\n        });\r\n        \r\n        if (existingReview) {\r\n          return res.status(400).json({ error: 'Vous avez dj laiss un avis pour ce produit' });\r\n        }\r\n        \r\n        const newReview = new Review({\r\n          product,\r\n          user: req.user._id,\r\n          rating,\r\n          comment,\r\n          isApproved: false // Par dfaut, les avis doivent tre approuvs\r\n        });\r\n        \r\n        await newReview.save();\r\n        \r\n        // Mettre  jour la note moyenne du produit\r\n        await updateProductRating(product);\r\n        \r\n        return res.status(201).json(newReview);\r\n        \r\n      default:\r\n        return res.status(405).json({ error: 'Mthode non autorise' });\r\n    }\r\n  } catch (error) {\r\n    console.error('Erreur API avis:', error);\r\n    return res.status(500).json({ error: 'Erreur serveur' });\r\n  }\r\n}\r\n\r\n// Fonction pour mettre  jour la note moyenne d'un produit\r\nasync function updateProductRating(productId) {\r\n  try {\r\n    // Rcuprer tous les avis approuvs pour ce produit\r\n    const reviews = await Review.find({\r\n      product: productId,\r\n      isApproved: true\r\n    });\r\n    \r\n    if (reviews.length === 0) {\r\n      return;\r\n    }\r\n    \r\n    // Calculer la note moyenne\r\n    const totalRating = reviews.reduce((sum, review) => sum + review.rating, 0);\r\n    const averageRating = totalRating / reviews.length;\r\n    \r\n    // Mettre  jour la note du produit\r\n    const Product = mongoose.models.Product;\r\n    if (Product) {\r\n      await Product.findByIdAndUpdate(productId, {\r\n        rating: averageRating\r\n      });\r\n    }\r\n  } catch (error) {\r\n    console.error('Erreur de mise  jour de la note du produit:', error);\r\n  }\r\n}\r\n\r\n// Appliquer le middleware d'authentification pour les mthodes POST\r\nexport default function(req, res) {\r\n  if (req.method === 'POST') {\r\n    return withAuth(handler)(req, res);\r\n  } else {\r\n    return handler(req, res);\r\n  }\r\n}\r\n","C:\\Users\\Louis Olivier\\Downloads\\ewebsite2-francise\\pages\\api\\upload\\image.js",[],[],"C:\\Users\\Louis Olivier\\Downloads\\ewebsite2-francise\\pages\\api\\users\\index.js",["886","887","888","889","890"],[],"import connectDB from '../../../src/config/db';\r\nimport mongoose from 'mongoose';\r\nimport bcrypt from 'bcryptjs';\r\n\r\n// Dfinition du schma User pour MongoDB\r\nconst UserSchema = new mongoose.Schema({\r\n  name: {\r\n    type: String,\r\n    required: true,\r\n    trim: true\r\n  },\r\n  email: {\r\n    type: String,\r\n    required: true,\r\n    unique: true,\r\n    trim: true,\r\n    lowercase: true\r\n  },\r\n  password: {\r\n    type: String,\r\n    required: true\r\n  },\r\n  role: {\r\n    type: String,\r\n    enum: ['user', 'admin'],\r\n    default: 'user'\r\n  },\r\n  profileImage: {\r\n    type: String,\r\n    default: ''\r\n  },\r\n  cloudinaryId: {\r\n    type: String,\r\n    default: ''\r\n  },\r\n  address: {\r\n    street: String,\r\n    city: String,\r\n    state: String,\r\n    postalCode: String,\r\n    country: String\r\n  },\r\n  phone: {\r\n    type: String,\r\n    default: ''\r\n  }